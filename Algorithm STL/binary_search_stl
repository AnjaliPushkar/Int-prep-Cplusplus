#include <iostream>
#include<algorithm>

using namespace std;

int main()
{
   int arr[] = {1,2,4,4,4,5,6};
   int n = sizeof(arr)/sizeof(int);
   int key;
   cin>>key;
   
   //search key
    bool present = binary_search(arr, arr+n, key);
    if(present){
        cout<<"present";
    }
    else{
        cout<<"absent";
    }
    return 0;
}
