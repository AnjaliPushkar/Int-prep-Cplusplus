#include <iostream>
#include<queue>
using namespace std;

int main() {
    queue<char>q;
    int freq[27] = {0};
    char ch;
    cin>>ch;
    while(ch != '.'){
        q.push(ch);
        freq[ch-'a']++;//for indexing
        while(!q.empty()){
            int ind = q.front() - 'a';
            if(freq[ind] > 1){
                q.pop();
            }
            else{
                cout<<q.front()<<endl;
                break;
            }
        }
        //if q is empty
        if(q.empty()){
            cout<<"-1"<<endl;
        }
           cin>>ch;
    }
    return 0;
}
/*
Input
aabccbcd.
Output
a
-1
b
b
b
-1
-1
d
*/